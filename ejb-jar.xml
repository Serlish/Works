<?xml version="1.0" encoding="UTF-8"?>
<ejb-jar xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         version="3.2"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/ejb-jar_3_2.xsd">
    
    <display-name>LabRabot6</display-name>
    <enterprise-beans>
        <session>
            <display-name>DBSessionBeans</display-name>
            <ejb-name>DBSessionBean</ejb-name>
            <remote>SessionBeans.DataBase</remote>
            <ejb-class>SessionBeans.DBSessionBean</ejb-class>
            <session-type>Stateless</session-type>
            <transaction-type>Container</transaction-type>
            <security-identity>
                <use-caller-identity/>
            </security-identity>
        </session>
    </enterprise-beans>

</ejb-jar>
